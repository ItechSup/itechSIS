{% extends 'ItechSupItechSisBundle::layout.html.twig' %}

{% block title %}Modification d'un jour de fermeture{% endblock %}

{% block content %}
    <div id="form">
        {{ form_start(edit_form) }}
        {{ form_errors(edit_form) }}
        {{ form_row(edit_form.reason) }}
        {{ form_row(edit_form.closingDate) }}
        <button type="submit" class="btn btn-default">Mettre à jour</button>
        <a class="btn btn-default" href="{{ path('room') }}">Retour à la liste</a>
        {{ form_end(edit_form) }}
    </div>
{% endblock %}

