{% extends 'ItechSupItechSisBundle::layout.html.twig' %}

{% block content%}
    <h1>Edition des sessions</h1>

    {{ form_start(edit_form) }}
    {{ form_errors(edit_form) }}
    <label>Date de début</label>
    {{ form_widget(edit_form.startDate) }}
    <label>Date de fin</label>
    {{ form_widget(edit_form.endDate) }}
    <label>Formation associée</label>
    {{ form_widget(edit_form.formation) }}
    {{ form_end(edit_form)}}

        <ul class="record_actions">
    <li>
        <a href="{{ path('session') }}">
            Retour à la liste
        </a>
    </li>
</ul>
{% endblock %}
