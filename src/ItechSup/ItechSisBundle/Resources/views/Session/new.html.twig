{% extends 'ItechSupItechSisBundle::layout.html.twig' %}

{% block content%}
    <h1 class="block-title">Création de session</h1>

    <div id="form">
    {{ form_start(form)}}
    {{ form_errors(form) }}
    <label class="form-label ">Date de début</label>
    {{ form_widget(form.startDate, {'attr': {'class': 'form-label'}}) }}
    <label class="form-label">Date de fin</label>
    {{ form_widget(form.endDate, {'attr': {'class': 'form-label'}}) }}
    {{ form_widget(form.formation, {'attr': {'class': ''}} ) }}
    {{ form_widget(form.submit, {'attr': {'class': 'new-entry button'}}) }}
    {{ form_end(form)}}
    </div>
        <ul class="record_actions">
    <li>
        <a href="{{ path('session') }}"class="update button ">
            Retour à la liste
        </a>
    </li>
</ul>
{% endblock %}
